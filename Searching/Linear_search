#include<stdio.h>
main()
{
	int n;
	printf("enter the size of the list to be searched");
	scanf("%d",&n);
	int a[n],i,j,k=0;
	for(i=0;i<n;i++)
	{
		printf("enter the %d element of the array",i+1);
		scanf("%d",&a[i]);
	}
	printf("enter the element to be searched");
	scanf("%d",&j);
	for(i=0;i<n;i++)
	{
		if(a[i]==j)
		{
			printf("%d element found at %d position",j,i+1);
		}
	}
	if(k==0)
	printf(" element %d not found",j);
}
